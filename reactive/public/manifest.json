{
  "manifest_version": 3,
  "name": "TobyClone",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "action": {
    "default_popup": ""
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
